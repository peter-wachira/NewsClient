<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent">


    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/textView8"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="8dp"
        style="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@android:color/black"
        android:padding="4dp"
        app:fontFamily="@font/roboto"
        android:textAlignment="viewStart"
        app:layout_constraintEnd_toStartOf="@+id/imageView3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="(Update) Iphone 13 Rumor New design ggyytggggggggggg ">

    </com.google.android.material.textview.MaterialTextView>


    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/textView6"
        style="@style/TextAppearance.AppCompat.Body1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/roboto"
        android:padding="4dp"
        android:textColor="@color/colorTextBlackSecondary"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        tools:text="3 Hours" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="Architect"
        android:padding="4dp"
        style="@style/TextAppearance.AppCompat.Body1"
        android:textColor="@color/colorTextBlackSecondary"
        android:layout_marginStart="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView6"
        android:drawableStart="@drawable/ic_eclipse"
        android:fontFamily="@font/roboto"
        android:drawablePadding="8dp"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/textView6" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="110dp"
        android:layout_height="90dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/rectange_radius_corners"
        android:src="@drawable/ic_eclipse"
        app:layout_constraintBottom_toBottomOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView8"
        app:srcCompat="@drawable/ic_small_image" />
</androidx.constraintlayout.widget.ConstraintLayout>